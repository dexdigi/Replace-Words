{
    "manifest_version": 2,
    "name": "Un-Transcender",
    "version": "0.1",
    "description": "makes backhandedly nice articles about non-white people more accurate. by @dexdigi",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end",
            "all_frames": true
        }
    ]
}